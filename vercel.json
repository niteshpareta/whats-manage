{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "buildCommand": false,
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/partner_sample.csv",
      "dest": "/partner_sample.csv"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|ico|json|csv))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 